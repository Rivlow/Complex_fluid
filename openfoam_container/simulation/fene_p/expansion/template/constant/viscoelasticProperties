rheology
{

                type FENE-P;
                rho              rho [1 -3 0 0 0 0 0] 1000.0;
                etaS             etaS [1 -1 -1 0 0 0 0] 0.0009;
                etaP             etaP [1 -1 -1 0 0 0 0] 0.001;
                lambda           lambda [0 0 1 0 0 0 0] 2.7;
                L2               L2 [0 0 0 0 0 0 0] 100.0;

}

